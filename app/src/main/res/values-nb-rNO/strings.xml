    <!-- Descriptions for app markets -->
    <string name="app_description_80_chars_max">Hjemmeskjermmoduler som viser hendelser fra valgte kalendere og oppgavelister</string>
    <string name="app_description_4000_chars_max_part01">Todo Agenda er hjemmeskjermmoduler til din Android-enhet.
       Hver modul har sine egne innstillinger og viser oppsatte lister over kalenderhendelser og oppgaver, sånn at du lett kan se over dine unnagjorte, nåværende og kommende hendelser.</string>
    <string name="app_description_4000_chars_max_part02">Egenskaper</string>
    <string name="app_description_4000_chars_max_part15">Ingen reklamer. Gratis og med åpen kildekode.</string>
    <string name="app_description_4000_chars_max_part03">Viser hendelser fra dine kalendere og oppgavelister for de valgte tidsrommene i for- og fremtid.</string>
    <string name="app_description_4000_chars_max_part04">Oppdateres automatisk når du legger til / sletter / endrer på en hendelse. Du kan også oppdatere listen umiddelbart.</string>
    <string name="app_description_4000_chars_max_part05">Begrens det til de kalenderene og oppgavelistene som du vil se i modulen.</string>
    <string name="app_description_4000_chars_max_part06">Lag flere moduler hvis du trenger det. Hver modul har sine egne innstillinger, inkludert oppsett, farger, filtre, valgte kalendere og oppgavelister.</string>
    <string name="app_description_4000_chars_max_part07">Tilpass bakgrunnsfargen, gjennomsiktigheten og tekstskyggen til forskjellige deler av modulene</string>
    <string name="app_description_4000_chars_max_part08">Bla gjennom hendelseslisten, bruk «Gå til \'I dag\'» for å gå umiddelbart tilbake til i dag.</string>
    <string name="app_description_4000_chars_max_part09">Tilpass modulens tekststørrelse.</string>
    <string name="app_description_4000_chars_max_part10">Modulene støtter full størrelsesendring og alternative oppsett.</string>
    <string name="app_description_4000_chars_max_part11">Indikatorer for alarmer og gjentakende hendelser.</string>
    <string name="app_description_4000_chars_max_part12">Lås fast tidssonen hvis du reiser til forskjellige tidssoner.</string>
    <string name="app_description_4000_chars_max_part13">Skru av moduloverskriften, dagsoverskrifter, hendelsesikoner, dager fra i dag, osv., og bare se det du behøver.</string>
    <string name="app_description_4000_chars_max_part16">Skjul duplikathendelser.</string>
    <string name="app_description_4000_chars_max_part17">Tilbakestill og gjenopprett innstillinger, så du kan klone moduler på den samme eller forskjellige enheter.</string>
    <string name="app_description_4000_chars_max_part14">Støtter Android ≥4.0. Støtter Android-nettbrett.</string>
