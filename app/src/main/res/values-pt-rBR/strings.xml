    <!-- Descriptions for app markets -->
    <string name="app_description_80_chars_max">Widgets exibindo eventos dos calendários selecionados</string>
    <string name="app_description_4000_chars_max_part01">Todo Agenda  é um widget para seu dispositivo Android.
        Cada widget tem suas próprias configurações e exibe a lista configurada de eventos e tarefas do calendário
        para que você possa facilmente ver seus eventos anteriores, atuais e futuros.</string>
    <string name="app_description_4000_chars_max_part02">Recursos</string>
    <string name="app_description_4000_chars_max_part15">Sem publicidade. Gratuito e Open Source.</string>
    <string name="app_description_4000_chars_max_part03">Exibe eventos de seus calendários 
        para os períodos selecionados no passado e no futuro.</string>
    <string name="app_description_4000_chars_max_part04">A lista é atualizada automaticamente quando um evento é adicionado/removido/modificado. Ou ela pode ser atualizada manualmente.</string>
    <string name="app_description_4000_chars_max_part05">Selecione apenas os calendários
        que você deseja ver no widget.</string>
    <string name="app_description_4000_chars_max_part06">Crie vários widgets, se você precisar. Cada widget tem suas próprias configurações,
        incluindo layouts, cores, filtros e calendários selecionados.</string>
    <string name="app_description_4000_chars_max_part07">Personalize cores de fundo e transparência
        de diferentes partes, sombreamento branco e preto para textos.</string>
    <string name="app_description_4000_chars_max_part08">Role pela lista de eventos.
        Use o botão \"Ir para Hoje\" para retornar ao dia atual.</string>
    <string name="app_description_4000_chars_max_part09">Ajuste o tamanho do texto do widget.</string>
    <string name="app_description_4000_chars_max_part10">Widget customizável com layouts alternativos.</string>
    <string name="app_description_4000_chars_max_part11">Indicadores para alertas e eventos recorrentes.</string>
    <string name="app_description_4000_chars_max_part12">Bloqueie o fuso horário quando viajar para outros fusos horários.</string>
    <string name="app_description_4000_chars_max_part13">Desative o cabeçalho do Widget, do Dia, ícones de eventos, dias a frente,
        etc. e veja apenas o que precisa.</string>
    <string name="app_description_4000_chars_max_part16">Ocultar eventos duplicados.</string>
    <string name="app_description_4000_chars_max_part17">Backup e restauração das configurações,
        clonando widgets aqui ou em outro aparelho.</string>
    <string name="app_description_4000_chars_max_part14">Suporte a Android 4+. Suporta tablets Android.</string>
